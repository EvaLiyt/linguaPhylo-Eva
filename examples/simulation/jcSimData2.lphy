data {
  L = 200;
  taxa = taxa(names=1:10);
}
model {
  ψ ~ Coalescent(theta=0.2, taxa=taxa);
  D ~ PhyloCTMC(tree=ψ, L=L, Q=jukesCantor());
  M ~ MissingSites(alignment=D, prob=0.75);
  F = filterMissingSites(alignment=M, threshold=0.75);
  var = classifySites(alignment=F, ignoreUnknown=true);
  const = classifySites(alignment=F, varSites=false, ignoreUnknown=true);
  V = copySites(ids=var, alignment=F);
  C = copySites(ids=const, alignment=F);
}
