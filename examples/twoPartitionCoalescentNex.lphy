data {
  file="examples/primate.nex";
  taxa = readTaxa(file= file);
  coding = readNexus(file = file, charset="coding");
  noncoding = readNexus(file = file, charset="noncoding");
  L1 = nchar(coding);
  L2 = nchar(noncoding);
}
model {
  Θ ~ LogNormal(meanlog=3, sdlog=1);
  ψ ~ Coalescent(theta=Θ, taxa=taxa);
  π ~ Dirichlet(conc=[2.0,2.0,2.0,2.0]);
  coding ~ PhyloCTMC(tree=ψ, L=L1, Q=f81(freq=π));
  noncoding ~ PhyloCTMC(tree=ψ, L=L2, Q=f81(freq=π));
}

